{% extends "layout/base.twig" %}

{% block head %}
    <style>
        table {
            font-size: 14px;
        }
        table caption {
            padding: 0.5em 1em;
        }
        table th {
            font-weight: bold;
            background: rgb(229, 231, 235);
        }
        table tbody td {
            font-family: monospace;
        }
        table td, table th {
            padding: 0.5em 1em;
            border: 1px solid rgb(209, 213, 219);
        }
